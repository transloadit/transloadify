{
  "name": "transloadify",
  "version": "0.2.1",
  "description": "CLI client for transloadit",
  "scripts": {
    "fix": "eslint src test --fix",
    "lint": "eslint src test",
    "deploy": "lanyon deploy",
    "web:build:production": "npm run web:inject && LANYON_ENV=production lanyon build",
    "web:build": "lanyon build",
    "web:inject": "./website/_scripts/inject.sh",
    "web:encrypt": "lanyon encrypt",
    "web:injecter": "nodemon --verbose --watch ./  --ignore 'website/*.md' --ignore website/_site --ignore website/.lanyon --ext html,json,md,js,css,png,jpg --exec 'npm run web:inject'",
    "web:install": "lanyon postinstall",
    "web:serve:production": "LANYON_ENV=production lanyon serve",
    "web:start": "npm-run-all --parallel web:injecter web:starter",
    "web:starter": "lanyon start",
    "release:major": "env SEMANTIC=major npm run release",
    "release:minor": "env SEMANTIC=minor npm run release",
    "release:patch": "env SEMANTIC=patch npm run release",
    "release": "npm version ${SEMANTIC:-patch} -m \"Release %s\" && npm run build && git push && git push --tags && npm publish",
    "test": "npm-run-all build mocha",
    "mocha": "NODE_ENV=test mocha --compilers js:babel-register --reporter spec test/",
    "build": "babel src/ -d lib/",
    "prestart": "npm run build",
    "start": "node bin/transloadify.js"
  },
  "lanyon": {
    "projectDir": "website"
  },
  "eslintConfig": {
    "extends": "standard",
    "env": {
      "node": true,
      "mocha": true
    }
  },
  "author": "Adrian Sinclair <adrian@transloadit.com>",
  "license": "MIT",
  "dependencies": {
    "node-watch": "^0.4.1",
    "q": "1.4.1",
    "transloadit": "1.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "chai": "3.5.0",
    "eslint": "^3.11.1",
    "eslint-config-standard": "6.2.1",
    "eslint-plugin-promise": "3.4.0",
    "eslint-plugin-standard": "2.0.1",
    "fakefile": "^0.0.8",
    "lanyon": "0.0.32",
    "mocha": "^3.2.0",
    "nodemon": "1.11.0",
    "npm-run-all": "^3.1.2",
    "source-map-support": "^0.4.6"
  },
  "babel": {
    "sourceMaps": "inline",
    "presets": [
      "es2015"
    ]
  }
}
